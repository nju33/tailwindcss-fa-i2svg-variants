  "scripts": {
    "build": "run-s build-ts build-css build-ts-for-jest",
    "build-ts": "tsc -p tsconfig.build.json",
    "build-ts-for-jest": "tsc -p tsconfig.jest-environment.json",
    "build-css": "postcss src/__tests__/fixtures/style.css -o src/__tests__/fixtures/style.build.css",
    "ci:gen-src-docs": "typedoc --plugin typedoc-plugin-markdown --out docs src --tsconfig tsconfig.build.json",
    "ci:test": "jest --config jest.config.js --collectCoverage",
    "dev": "tsc -p tsconfig.build.json --watch",
    "pretest": "yarn build",
    "test": "jest --config jest.config.js"
  },
